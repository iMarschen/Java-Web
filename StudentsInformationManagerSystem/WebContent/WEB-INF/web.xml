  </servlet>
  <servlet-mapping>
    <servlet-name>addStuInfo</servlet-name>
    <url-pattern>/addStuInfoReq</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>findstuInfo</servlet-name>
    <servlet-class>servlet.StudentInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>findstuInfo</servlet-name>
